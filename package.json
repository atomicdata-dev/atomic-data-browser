{
  "devDependencies": {
    "@esm-bundle/chai": "4.3.4",
    "@playwright/test": "^1.25.2",
    "@testing-library/react": "12.1.2",
    "@types/chai": "^4.2.22",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.4",
    "@types/react": "^18.0.20",
    "@types/react-router-dom": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitejs/plugin-react": "^1.3.0",
    "@web/test-runner": "^0.13.20",
    "@web/test-runner-playwright": "^0.8.8",
    "@web/test-runner-puppeteer": "^0.10.2",
    "chai": "^4.3.4",
    "eslint": "8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^29.0.2",
    "playwright": "^1.16.1",
    "prettier": "2.4.1",
    "prettier-plugin-jsdoc": "^0.3.24",
    "react": "^18.2.0",
    "ts-jest": "^28.0.8",
    "typedoc": "^0.22.15",
    "typescript": "^4.8",
    "vite": "^3.0.5"
  },
  "name": "@tomic/root",
  "private": true,
  "scripts": {
    "lint": "pnpm workspaces foreach run lint",
    "lint-fix": "pnpm workspaces foreach run lint-fix",
    "build": "pnpm run -r build",
    "build-server": "pnpm run build && cp -r data-browser/dist/assets/ ../atomic-data-rust/server/app_assets/dist/ && cp data-browser/tests/e2e.spec.ts ../atomic-data-rust/server/e2e_tests/e2e-generated.spec.ts",
    "test": "pnpm run -r test",
    "test-query": "pnpm workspace @tomic/data-browser run test-query",
    "publish": "pnpm workspaces foreach --no-private npm publish",
    "start": "pnpm run -r --parallel start",
    "start-browser": "pnpm workspace @tomic/data-browser start",
    "start-lib": "pnpm workspace @tomic/lib watch",
    "typedoc": "typedoc . --options typedoc.json"
  },
  "workspaces": {
    "packages": [
      "lib",
      "react",
      "data-browser"
    ],
    "packageManager": "pnpm@7.11.0"
  },
  "dependencies": {
    "eslint-plugin-import": "^2.26.0"
  }
}
